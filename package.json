  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-media",
    "stop": "egg-scripts stop --title=egg-server-media",
    "dev": "egg-bin dev",
