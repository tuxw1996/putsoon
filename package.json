    "egg-passport": "^2.1.1",
    "egg-passport-jwt": "^1.0.0",
    "egg-scripts": "^2.11.0",
    "egg-swagger-doc": "^2.3.2",
    "fibers": "^4.0.2",
    "file-type": "^14.1.2",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "md5-file": "^4.0.0",
    "mkdir-recursive": "^0.4.0",
    "rmdir-recursive": "0.0.1",
    "save": "^2.4.0",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.1.1",
    "unique-string": "^2.0.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "donkey-plugin-slim": "file:../donkey-plugin-slim",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
